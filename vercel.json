{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/pv/$1",
      "has": [
        {
          "type": "host",
          "value": "lunalean.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/pv/$1",
      "has": [
        {
          "type": "host",
          "value": "www.lunalean.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/lunalean10/$1",
      "has": [
        {
          "type": "host",
          "value": "app.lunalean.com"
        }
      ]
    }
  ]
}